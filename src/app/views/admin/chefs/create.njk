        <div class="header">
            <h3>Criando chef</h3>
            <button type="submit">Salvar chef</button>
        </div>
        <p>Nome</p>
        <input type="text" name="name">
        <p>Avatar</p>
        <input type="url" name="avatar_url">
    </form>
{% endblock content %}